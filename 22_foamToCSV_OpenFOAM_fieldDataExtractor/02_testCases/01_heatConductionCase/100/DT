FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "100";
    object      DT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    north
    {
        type            calculated;
        value           uniform 1;
    }
    south
    {
        type            calculated;
        value           uniform 1;
    }
    west
    {
        type            calculated;
        value           uniform 1;
    }
    east
    {
        type            calculated;
        value           uniform 1;
    }
    top
    {
        type            empty;
    }
    bottom
    {
        type            empty;
    }
}


